    "def ecdf(C_califfisica):\n",
    "    #n numero de datos\n",
    "    n= len(C_califfisica)\n",
    "    \n",
    "    # ordenar a x\n",
    "    x=np.sort(C_califfisica)\n",
    "    \n",
    "    #datos de y\n",
    "    y=np.arange(1,n+1)/n\n",
    "    \n",
    "    return x,y\n",
    "#calcular ecdf\n",
    "x_1 , y_1 = ecdf(C_califfisica)\n",
    "\n",
    "plt.plot(x_1 , y_1 ,marker='',linestyle ='solid')\n",
    "plt.title(\"distribucion empirica\")\n",
    "plt.show()\n"
